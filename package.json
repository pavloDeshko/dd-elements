{
  "name": "dd-elements",
  "version": "0.1.0",
  "description": "D3-style alternative to JSX for React.js",
  "keywords": [
    "jsx",
    "no-jsx",
    "react",
    "data-driven",
    "d3"
  ],
  "homepage": "https://github.com/pavloDeshko/dd-elements",
  "main": "index.js",
  "files": [
    "/lib/*"
  ],
  "scripts": {
    "test": "npm run build && jest",
    "test:ts": "jest test.ts",
    "build": "node_modules/.bin/babel src --out-dir lib",
    "size": "size-limit"
  },
  "author": "Pavlo Deshko <pavlodeshko92@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@size-limit/preset-small-lib": "^5.0.5",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.27",
    "@types/react-test-renderer": "^17.0.1",
    "babel-preset-minify": "^0.5.1",
    "jest": "^27.4.3",
    "react": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "size-limit": "^5.0.5",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": "*"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "ie": "9"
          }
        }
      ],
      "minify"
    ],
    "plugins": []
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    }
  },
  "size-limit": [
    {
      "path": "index.js"
    }
  ],
  "dependencies": {}
}
