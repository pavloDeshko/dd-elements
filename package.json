{
  "name": "dd-elements",
  "version": "0.1.0",
  "description": "D3-style alternative to JSX for React.js",
  "keywords": [
    "jsx",
    "no-jsx",
    "react",
    "data-driven",
    "d3"
  ],
  "homepage": "https://github.com/pavloDeshko/dd-elements",
  "author": "Pavlo Deshko <pavlodeshko92@gmail.com>",
  "license": "MIT",
  "main": "./lib/index",
  "scripts": {
    "test": "jest test.ts",
    "build": "tsc",
    "size": "size-limit",
    "test:legacy": "jest ./legacy/test.js"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.4",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.27",
    "@types/react-test-renderer": "^17.0.1",
    "jest": "^27.4.3",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "size-limit": "^7.0.4",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "ie": "9"
          }
        }
      ],
      "minify"
    ],
    "plugins": []
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    }
  },
  "size-limit": [
    {
      "path": "./lib/index.js"
    }
  ]
}
